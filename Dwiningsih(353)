public class AnandaDwiNingsih {

    public static void main(String[] args) {
        String fullname = "Dwi Ningsih";
        String nim = "2213010353";
        String team = "Kelompok Java";
        String role = "Designer";

        System.out.println("Halo, perkenalkan nama saya " + fullname + ".");
        System.out.println("Nim saya = " + nim + ".");
        System.out.println("Saya tergabung dalam kelompok " + team + ".");
        System.out.println("Peran saya disini sebagai " + role + ".");
    }

}
  
